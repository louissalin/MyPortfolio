<header>
    <h1>Photography Portfolio</h1>
    <p>A collection of beautiful moments captured through the lens</p>
</header>

<main class="gallery">
    <!-- Row 1: Tall height -->
    <div class="gallery-row" style="height: 400px;">
        <div class="photo" style="background-color: #FF6B6B;"></div>
        <div class="photo" style="background-color: #4ECDC4; flex: 1.5;"></div>
        <div class="photo" style="background-color: #45B7D1;"></div>
    </div>

    <!-- Row 2: Medium height -->
    <div class="gallery-row" style="height: 300px;">
        <div class="photo" style="background-color: #96CEB4; flex: 0.8;"></div>
        <div class="photo" style="background-color: #FFEEAD;"></div>
        <div class="photo" style="background-color: #D4A5A5; flex: 1.2;"></div>
        <div class="photo" style="background-color: #9AC1D9;"></div>
    </div>

    <!-- Row 3: Short height -->
    <div class="gallery-row" style="height: 250px;">
        <div class="photo" style="background-color: #FFD93D; flex: 1.3;"></div>
        <div class="photo" style="background-color: #6C5B7B;"></div>
        <div class="photo" style="background-color: #C06C84;"></div>
    </div>

    <!-- Row 4: Medium height -->
    <div class="gallery-row" style="height: 300px;">
        <div class="photo" style="background-color: #F8B195;"></div>
        <div class="photo" style="background-color: #355C7D; flex: 1.4;"></div>
        <div class="photo" style="background-color: #99B898;"></div>
        <div class="photo" style="background-color: #2A363B; flex: 0.7;"></div>
    </div>

    <!-- Row 5: Tall height -->
    <div class="gallery-row" style="height: 450px;">
        <div class="photo" style="background-color: #A8E6CE; flex: 1.2;"></div>
        <div class="photo" style="background-color: #DCEDC2;"></div>
        <div class="photo" style="background-color: #FFD3B5; flex: 0.8;"></div>
        <div class="photo" style="background-color: #FFAAA6;"></div>
        <div class="photo" style="background-color: #FF8C94; flex: 1.1;"></div>
    </div>
</main>